{
  "manifest_version": 3,
  "name": "YouTube Music Volume Control",
  "version": "1.0",
  "description": "Improves volume control for YouTube Music with finer adjustments at lower volumes",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon32.png",
    "128": "icons/icon192.png"
  },
  "permissions": ["activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": ["*://music.youtube.com/*"],
      "js": ["content.js"]
    }
  ]
}
